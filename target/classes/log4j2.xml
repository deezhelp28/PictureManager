<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error" name="picturemanager" packages="">
	<Appenders>
		<Console name="stdout" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="error">
			<AppenderRef ref="stdout" />
		</Root>
	</Loggers>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="stdout" />
		</Root>
	</Loggers>
</Configuration>